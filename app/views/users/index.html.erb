<main>
 <nav>
    <div class="nav-wrapper">
      <div class="col s12">
        <a href="#!" class="breadcrumb">First</a>
        <a href="#!" class="breadcrumb">Second</a>
        <a href="#!" class="breadcrumb">Third</a>
      </div>
    </div>
  </nav>
  
      
 <div class="row"> 
   <div class="col s12 m12">   
   <div class="card-panel grey lighten-5 z-depth-1">
          <%#= link_to 'New user', new_user_path, :class => 'btn-floating btn-small waves-effect waves-light ' %>
          <a class="waves-effect waves-light btn-floating modal-trigger" href="#modal1">Import</a>
          <%= link_to "Excel", users_path(format: "xls") , :class => 'btn-floating btn-small waves-effect waves-light ' %>
        </div>
    </div>
    <div class="col s12 m12">    
        <div class="card-panel grey lighten-5 z-depth-1">
        
        <table class="bordered highlight" id="users">
          <thead>
            <tr>
              <th>Name</th>
              <th>Actions</th>
            </tr>
          </thead>

          <tbody>
            <% @users.each do |user| %>
              <tr>
                <td><%= user.email %></td>
               
                <td><%#= link_to 'Edit', edit_user_path(user) %>
                <%#= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
        </div>
      
    </div>
  </div>
</main>  



<script type="text/javascript">

      $( document ).ready(function() {     
        $('#users').dynatable();
      });
</script>
    